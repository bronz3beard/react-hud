{
  "name": "react-heads-up-display",
  "version": "0.1.4",
  "private": false,
  "homepage": "https://github.com/bronz3beard/react-hud",
  "author": "bronz3beard <exempli.gratia.webdesign@gmail.com> (https://www.eg-web-design.com/)",
  "keywords": [
    "react",
    "HUD",
    "Head Up Display",
    "Feature",
    "View",
    "Video",
    "Images",
    "showcase"
  ],
  "main": "dist/index.js",
  "module": "dist/index.js",
  "scripts": {
    "start": "react-scripts start",
    "build": "rm -rf dist && NODE_ENV=production babel src/lib --out-dir dist --copy-files",
    "build-og": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "watch:css": "postcss ./src/assets/tailwind.css -o ./src/assets/main.css",
    "watch": "chokidar \"./src/**/*.js\" -c \"npm run watch:css\"",
    "dev": "concurrently \"npm run watch\" \"npm run start\""
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@testing-library/jest-dom": "latest",
    "@testing-library/react": "latest",
    "@testing-library/user-event": "latest",
    "autoprefixer": "latest",
    "postcss": "latest",
    "react": "latest",
    "react-dom": "latest",
    "react-scripts": "latest",
    "web-vitals": "latest"
  },
  "devDependencies": {
    "react-heads-up-display": "^0.1.2",
    "@fortawesome/free-brands-svg-icons": "latest",
    "@fortawesome/free-solid-svg-icons": "latest",
    "@fortawesome/react-fontawesome": "latest",
    "@babel/cli": "latest",
    "@babel/preset-react": "latest",
    "autoprefixer": "latest",
    "chokidar-cli": "latest",
    "concurrently": "latest",
    "postcss": "latest",
    "postcss-cli": "latest",
    "tailwindcss": "latest"
  }
}
